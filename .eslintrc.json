{
  "env": {
    "browser": true,
    "es2020": true
  },

  "extends": ["eslint:recommended",  "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },

  "rules": {
    "no-undef": "off",
    "no-extra-boolean-cast": "off"
  }
}
